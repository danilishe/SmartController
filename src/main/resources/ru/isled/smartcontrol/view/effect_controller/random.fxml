<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox spacing="5.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <GridPane alignment="CENTER_LEFT" hgap="10.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES"/>
                <ColumnConstraints hgrow="SOMETIMES"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <Label text="Максимальная яркость"/>
            <HBox alignment="CENTER_LEFT" spacing="5.0" GridPane.columnIndex="1">
                <Slider fx:id="maxBright" value="255.0" max="255.0"/>
                <Label fx:id="maxBrightLabel" minWidth="30.0" text="255"/>
            </HBox>
            <Label layoutX="10.0" layoutY="17.0" text="Минимальная яркость" GridPane.rowIndex="1"/>
            <HBox alignment="CENTER_LEFT" spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                <Slider fx:id="minBright" max="255.0" value="0"/>
                <Label fx:id="minBrightLabel" minWidth="30.0" text="0"/>
            </HBox>
        </GridPane>
        <CheckBox fx:id="onlyEmpty" mnemonicParsing="false" text="Менять только пустые"/>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</VBox>
